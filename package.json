    "devDependencies": {
        "@types/node": "^14.14.31",
        "@types/node-fetch": "^2.5.8",
        "@types/source-map-support": "^0.5.3",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.16.1",
        "eslint": "^7.21.0",
        "eslint-plugin-header": "^3.1.1",
        "eslint-plugin-promise": "^4.3.1",
        "prettier": "^2.2.1",
